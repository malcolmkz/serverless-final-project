{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "todo-request",
    "type": "object",
    "properties": {
      "createdAt": {
        "type": "string"
      },
      "todoName": {
        "type": "string",
        "minLength": 1
      },
      "dueDate": {
        "type": "string"
      },
      "done": {
        "type": "boolean"
      },
      "attachmentUrl": {
        "type": "string"
      }
    },
    "required": [
        "todoName"
    ],
    "additionalProperties": false
  }